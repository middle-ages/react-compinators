{
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "eslint.useFlatConfig": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.options": {
    "overrideConfigFile": "config/eslint.config.ts"
  },
  "eslint.workingDirectories": [
    {
      "directory": "."
    }
  ],
  "eslint.useESLintClass": true,
  "files.autoSave": "onFocusChange",
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.formatOnSave": true,
  "notebook.formatOnSave.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "assumeprop",
    "assumeprops",
    "autodocs",
    "combinators",
    "Combinators",
    "compinators",
    "contramap",
    "depcruise",
    "displayname",
    "Eeuo",
    "Eilam",
    "Endo",
    "expando",
    "mapprop",
    "mapprops",
    "modprop",
    "modstyle",
    "Monoid",
    "nodenext",
    "omitprops",
    "pointcuts",
    "prismjs",
    "renameprop",
    "renameprops",
    "serviceworker",
    "sonarjs",
    "storysource",
    "Symbola",
    "tailwindcss",
    "unfoldprop",
    "unionvariant",
    "unionvariants",
    "vite",
    "withdefaultstyle",
    "withstyle",
    "withvariants"
  ],
  "eslint.probe": [
    "typescript",
    "typescriptreact",
    "html",
    "mdx",
    "markdown",
    "json"
  ],
  "typescript.suggest.autoImports": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "markdownlint.config": {
    "MD033": false
  }
}
